   "source": [
    "x = V(np.array([[1,2,3],[4,5,6]], dtype='float'), requires_grad=True)\n",
    "p = V(np.array([2], dtype='float'))\n",
    "y = x ** p\n",
    "z = mean(y, axis=1)\n",
    "z.backward()"
